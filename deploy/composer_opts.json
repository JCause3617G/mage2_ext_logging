{
  "require": {
    "praxigento/composer_plugin_templates": "^1.0",
    "praxigento/mage2_ext_logging": "dev-master"
  },
  "require-dev": {
    "satooshi/php-coveralls": "dev-master",
    "mockery/mockery": "^0.9.4"
  },
  "autoload-dev": {
    "psr-4": {
      "Magento\\TestFramework\\": "dev/tests/integration/framework/Magento/TestFramework"
    }
  },
  "repositories": {
    "satooshi/php-coveralls": {
      "type": "vcs",
      "url": "https://github.com/flancer64/php-coveralls"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "praxigento_templates_config": [
      "../deploy/templates.cfg.json",
      "../templates.vars.work.json"
    ]
  }
}